{
	"name": "walkscape-planner",
	"private": true,
	"version": "0.0.1",
	"homepage": "https://iceman98.github.io/walkscape-planner",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
